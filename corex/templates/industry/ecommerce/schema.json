{
  "name": "ecommerce",
  "title": "E-commerce Platform",
  "description": "Template for online stores, marketplaces, and e-commerce platforms",
  "version": "1.0.0",
  "modules": [
    {
      "name": "auth",
      "required": true,
      "description": "User authentication and authorization"
    },
    {
      "name": "products",
      "required": true,
      "description": "Product catalog and management"
    },
    {
      "name": "orders",
      "required": true,
      "description": "Order processing and management"
    },
    {
      "name": "payments",
      "required": true,
      "description": "Payment processing integration"
    },
    {
      "name": "inventory",
      "required": false,
      "description": "Inventory tracking and management"
    },
    {
      "name": "reviews",
      "required": false,
      "description": "Product reviews and ratings"
    },
    {
      "name": "coupons",
      "required": false,
      "description": "Discount coupons and promotions"
    }
  ],
  "dependencies": {
    "python": ">=3.8",
    "django": ">=3.2",
    "packages": [
      "django-rest-framework>=3.12.0",
      "stripe>=2.60.0",
      "pillow>=8.3.0",
      "django-countries>=7.2.0"
    ]
  }
}