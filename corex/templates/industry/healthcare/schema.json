{
  "name": "healthcare",
  "title": "Healthcare",
  "description": "Template for clinics, hospitals, and healthcare management systems",
  "version": "1.0.0",
  "modules": [
    {
      "name": "auth",
      "required": true,
      "description": "User authentication and authorization"
    },
    {
      "name": "patients",
      "required": true,
      "description": "Patient management and records"
    },
    {
      "name": "appointments",
      "required": true,
      "description": "Appointment scheduling and management"
    },
    {
      "name": "medical_records",
      "required": true,
      "description": "Electronic medical records"
    },
    {
      "name": "billing",
      "required": true,
      "description": "Medical billing and insurance"
    },
    {
      "name": "staff",
      "required": false,
      "description": "Medical staff management"
    },
    {
      "name": "inventory",
      "required": false,
      "description": "Medical inventory tracking"
    }
  ],
  "dependencies": {
    "python": ">=3.8",
    "django": ">=3.2",
    "packages": [
      "django-rest-framework>=3.12.0",
      "pillow>=8.3.0",
      "django-phonenumber-field>=6.0.0",
      "reportlab>=3.6.0"
    ]
  }
}