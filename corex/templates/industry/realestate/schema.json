{
  "name": "realestate",
  "title": "Real Estate",
  "description": "Template for real estate agencies, property management, and listings",
  "version": "1.0.0",
  "modules": [
    {
      "name": "auth",
      "required": true,
      "description": "User authentication and authorization"
    },
    {
      "name": "properties",
      "required": true,
      "description": "Property listings and details"
    },
    {
      "name": "listings",
      "required": true,
      "description": "Property listings management"
    },
    {
      "name": "agents",
      "required": true,
      "description": "Real estate agents management"
    },
    {
      "name": "transactions",
      "required": true,
      "description": "Property transactions and deals"
    },
    {
      "name": "clients",
      "required": false,
      "description": "Client management and relationships"
    },
    {
      "name": "calendar",
      "required": false,
      "description": "Showing calendar and appointments"
    }
  ],
  "dependencies": {
    "python": ">=3.8",
    "django": ">=3.2",
    "packages": [
      "django-rest-framework>=3.12.0",
      "pillow>=8.3.0",
      "django-countries>=7.2.0",
      "django-phonenumber-field>=6.0.0"
    ]
  }
}